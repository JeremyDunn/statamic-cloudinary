{
    "name": "doeanderson/statamic-cloudinary",
    "description": "Cloudinary addon for Statamic 3",
    "require": {
        "php": "^8.0 || ^8.1",
        "cloudinary-labs/cloudinary-laravel": "^2.0",
        "laravel/framework": "^9.0 || ^10.0",
        "statamic/cms": "^4.0",
        "stillat/proteus": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DoeAnderson\\StatamicCloudinary\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Cloudinary",
            "description": "Coudinary addon"
        },
        "laravel": {
            "providers": [
                "DoeAnderson\\StatamicCloudinary\\ServiceProvider"
            ]
        }
    }
}
